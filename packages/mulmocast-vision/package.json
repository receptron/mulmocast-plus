{
  "name": "mulmocast-vision",
  "version": "1.0.8",
  "description": "Easy and stylish presentation slide generator",
  "mcpName": "io.github.isamu/mulmocast-vision",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "bin": {
