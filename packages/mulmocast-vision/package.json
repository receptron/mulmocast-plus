  },
  "files": [
    "./lib",
    "./html"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src tests",
    "format": "prettier --write '{src,tests,html}/**/*.{ts,html}'",
    "test": "echo nothing"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/receptron/mulmocast-plus.git"
  },
  "author": "Satoshi Nakajima, Isamu Arimoto",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/receptron/mulmocast-plus/issues"
  },
  "homepage": "https://github.com/receptron/mulmocast-plus#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "nunjucks": "^3.2.4",
    "pdfkit": "^0.17.2",
    "puppeteer": "^24.37.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/nunjucks": "^3.2.6",
    "@types/pdfkit": "^0.17.4",
    "cross-env": "^10.1.0"
  }
